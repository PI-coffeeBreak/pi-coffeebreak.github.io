<script>
  // Seleciona todos os controladores de tema
  const themeControllers = document.querySelectorAll('.theme-controller') as NodeListOf<HTMLInputElement>;
  
  // Define o estado inicial baseado no tema armazenado
  themeControllers.forEach(controller => {
    if (localStorage.getItem('theme') === 'coffeeDark') {
      controller.checked = true;
    } else {
      controller.checked = false;
    }
  });

  // Adiciona listeners para sincronizar os controladores
  themeControllers.forEach(controller => {
    controller.addEventListener('change', (e) => {
      const isChecked = (e.target as HTMLInputElement).checked;
      themeControllers.forEach(ctrl => {
        ctrl.checked = isChecked;
      });
    });
  });

  // Remove a classe 'hidden' dos elementos swap apÃ³s carregar os listeners
  document.querySelectorAll('.swap').forEach(swap => {
    swap.classList.remove('hidden');
  });
</script>